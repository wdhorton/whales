<h1>Sign Up!</h1>

<form action='/users' method='POST'>
  <input type='hidden' name="authenticity_token" value="<%= form_authenticity_token %>">

  <label>Username
    <input type="text" name="user[username]">
  </label>

  <label>Password
    <input type="password" name="user[password]">
  </label>

  <button>Sign Up</button>
</form>
